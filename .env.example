# Copy this file to .env and add your API keys

# Google OAuth 2.0 credentials for desktop authentication
GOOGLE_DESKTOP_CLIENT_ID=your_desktop_client_id_here
GOOGLE_CLIENT_SECRET=your_client_secret_here

# API Keys for AI services
GEMINI_API_KEY=your_gemini_api_key_here
GOOGLE_GENAI_API_KEY=your_genai_api_key_here

# Jarvis API Configuration
JARVIS_API_URL=https://api.jarvis.cx
JARVIS_API_KEY=your_jarvis_api_key_here

# Stack Authentication
AUTH_API_URL=https://app.stack-auth.com/api/v1
STACK_PROJECT_ID=your_stack_project_id_here
STACK_PUBLISHABLE_CLIENT_KEY=your_stack_client_key_here

# Firebase Configuration (optional override)
